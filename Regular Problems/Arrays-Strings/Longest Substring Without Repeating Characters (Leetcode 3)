class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:

        maxCount = 0
        seen = set()
        left = 0

        for right in range(len(s)):
            while s[right] in seen:
                seen.remove(s[left])
                left += 1
            seen.add(s[right])

            maxCount = max(maxCount, right - left + 1)

        return maxCount

# Time Complexity is O(n)
# Space Complexity is O(n)
