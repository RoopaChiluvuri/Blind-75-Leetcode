class Solution:
    def rotateString(self, s: str, goal: str) -> bool:
        if len(s) != len(goal): return False

        t = s + s
        if goal in t:
            return True
        else:
            return False
        
# Avg Time Complexity is O(n)
# Worst Time Complexity is O(n^2)

# Space Complexity is O(n)
